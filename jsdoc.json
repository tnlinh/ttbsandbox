{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["lib/ttbsdk.js"]
  },
  "plugins": [],
  "templates": {
    "applicationName": "TitleToolBox SDK",
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "customOptions": {
    "rootDir": "ttbsandbox",
    "stylesheet": "/main.css",
    "footer": "Copyright Â© 2017 <a href=\"http://www.benutech.com/\" target=\"_blank\" title=\"Benutech official website\">Benutech Inc.</a> All rights reserved."
  },
  "opts": {
    "template": "jaguarjs-jsdoc",
    "template1": "templates/default",
    "encoding": "utf8",
    "destination": "./dist/",
    "recurse": true
  }
}