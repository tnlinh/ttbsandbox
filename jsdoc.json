{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": ["src/sandbox.js"]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "template": "jaguarjs-jsdoc",
    "template1": "templates/default",
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true
  }
}